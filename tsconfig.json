{
  "compilerOptions": {
     "lib": [
        "es5",
        "es6",
     ],
     "types": ["node"],
     "target": "es2017",
     "module": "commonjs",
     "moduleResolution": "node",
     "outDir": "./build",
     "emitDecoratorMetadata": true,
     "experimentalDecorators": true,
     "sourceMap": true,
     "esModuleInterop": true,
     "strictNullChecks": true
  }
}